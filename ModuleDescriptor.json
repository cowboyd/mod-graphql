{
  "id": "mod-graphql-0.1.0",
  "name": "GraphQL API for FOLIO",
  "provides": [
    {
      "id": "graphql",
      "version": "0.0.0",
      "handlers" : [
        {
          "methods" : [ "POST" ],
          "pathPattern" : "/graphql"
        }
      ]
    }
  ],
  "launchDescriptor": {
    "dockerImage": "mod-graphql:0.1.0",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 268435456,
        "PortBindings": { "3001/tcp": [ { "HostPort": "%p" } ] }
      }
    }
  }
}
