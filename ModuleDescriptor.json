{
  "id": "mod-graphql-0.1.0",
  "name": "GraphQL API for FOLIO",
  "provides": [
    {
      "id": "graphql",
      "version": "0.0.0",
      "handlers" : [
        {
          "methods" : [ "POST" ],
          "pathPattern" : "/graphql"
        }
      ]
    }
  ],
  "launchDescriptor": {
    "dockerImage": "mod-graphql:0.1.0",
    "dockerArgs": {
      "HostConfig": { "PortBindings": { "3000/tcp":  [{ "HostPort": "%p" }] } }
    },
    "dockerPull" : false
  }
}
