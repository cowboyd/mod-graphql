{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "discoveryDisplayName": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean"
    },
    "institutionId": {
      "type": "string"
    },
    "campusId": {
      "type": "string"
    },
    "libraryId": {
      "type": "string"
    },
    "details": {
      "type": "string"
    },
    "metadata": {
      "type": "object",
      "$ref": "raml-util/schemas/metadata.schema",
      "readonly": true
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "code",
    "institutionId",
    "campusId",
    "libraryId"
  ]
}
